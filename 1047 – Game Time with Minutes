#include <stdio.h>

int main() {
    int h1, m1, h2, m2;
    scanf("%d %d %d %d", &h1, &m1, &h2, &m2);

    int start = h1 * 60 + m1;
    int end = h2 * 60 + m2;

    int duration = end - start;

    // If end time is earlier or equal â†’ next day
    if (duration <= 0) {
        duration += 24 * 60;
    }

    int hour = duration / 60;
    int minute = duration % 60;

    printf("O JOGO DUROU %d HORA(S) E %d MINUTO(S)\n", hour, minute);

    return 0;
}
