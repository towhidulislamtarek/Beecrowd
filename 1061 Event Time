#include <stdio.h>

int main() {
    int d1, h1, m1, s1;
    int d2, h2, m2, s2;

    // Input
    scanf("Dia %d", &d1);
    scanf("%d : %d : %d", &h1, &m1, &s1);

    scanf("Dia %d", &d2);
    scanf("%d : %d : %d", &h2, &m2, &s2);

    // Convert all to seconds
    int start = s1 + m1*60 + h1*3600 + d1*86400;
    int end   = s2 + m2*60 + h2*3600 + d2*86400;

    int diff = end - start;

    // Convert back to days, hours, minutes, seconds
    int days = diff / 86400;
    diff %= 86400;

    int hours = diff / 3600;
    diff %= 3600;

    int minutes = diff / 60;
    int seconds = diff % 60;

    // Output
    printf("%d dia(s)\n", days);
    printf("%d hora(s)\n", hours);
    printf("%d minuto(s)\n", minutes);
    printf("%d segundo(s)\n", seconds);

    return 0;
}
