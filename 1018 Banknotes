#include <stdio.h>

int main() {
    int N;
    int notes[7] = {100, 50, 20, 10, 5, 2, 1};
    int count[7];

    // Read the value
    scanf("%d", &N);

    int remaining = N;

    // Calculate the number of each banknote
    for (int i = 0; i < 7; i++) {
        count[i] = remaining / notes[i];
        remaining = remaining % notes[i];
    }

    // Print the results
    printf("%d\n", N);
    for (int i = 0; i < 7; i++) {
        printf("%d nota(s) de R$ %d,00\n", count[i], notes[i]);
    }

    return 0;
}
